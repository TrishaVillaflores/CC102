#include <iostream>
using namespace std;
int main() {

    int rows = 5;
    for (int i = 0; i < rows; ++i) {
        for (int m = 0; m < rows * 2; ++m) {
        if (i % 2 == 0) {
        cout << "^ ";
        } else {
        cout << " ^";
       }
        }
        cout << endl;
    }
    return 0;
}