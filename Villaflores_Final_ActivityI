#include <iostream>
using namespace std;

int main() {
    int rows = 5;

    for (int i = 1; i <= rows; i++) {
        for (int j = 1; j <= 10; j++) {
            cout << " ";
        }
        for (int j = 1; j < i; j++) {
            cout << "^";
        }
        cout << "*" << endl;
    }
      for (int i = 1; i <= 2; i++) {
        for (int j = 1; j <= 10; j++) {
            cout << "#";
        }
        for (int k = 1; k <= 5; k++) {
            cout << "^";
        }
        cout << "*" << endl;
    }

    for (int i = rows - 0; i >= 1; i--) {
        for (int j = 1; j <= 10; j++) {
            cout << " ";
        }

        for (int j = 1; j < i; j++) {
            cout << "^";
        }

        cout << "*" << endl;
    }
    return 0;
}