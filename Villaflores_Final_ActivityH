#include <iostream>
using namespace std;

int main() {
    int n = 8; 
    for (int i = 0; i <= n; ++i) {
    for (int m = 0; m < i; ++m) {
    cout << "#";
    }
    cout << "^" << endl;
    }
    for (int i = n - 1; i >= 0; --i) {
        for (int m = 0; m < i; ++m) {
        cout << "#";
     }
     cout << "^" << endl;
    }
    return 0;
}